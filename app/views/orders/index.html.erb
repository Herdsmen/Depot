<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<h1>Listing orders</h1>

<table>
  <tr>
    <th>Id</th>
    <td></td>
    <th>Create Time</th>
    <td></td>
    <th>Status</th>
  </tr>

<% @orders.each do |order| %>
  <tr>
    <td><%= order.id %></td>
    <td></td>
    <td><%= order.created_at.to_s(:db) %></td>
    <td></td>
    <td><%= order.show_status %></td>
    <td></td>
    <td><%= link_to 'Show', order %></td>
    <td><%= link_to 'Destroy', order, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<%= link_to 'show_unshipped_orders',admin_url%>

<br />


<p><%= will_paginate @orders %></p>