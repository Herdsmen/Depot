<h1>Listing carts</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @carts.each do |cart| %>
  <tr>
    <td><%= link_to t('.show'), cart %></td>
    <td><%= link_to t('.edit'), edit_cart_path(cart) %></td>
    <td><%= link_to t('.destroy'), cart, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to t('.new_cart'), new_cart_path %>
